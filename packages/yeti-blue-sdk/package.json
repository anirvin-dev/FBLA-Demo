{
  "name": "@yeti-robotics/yeti-blue-sdk",
  "version": "0.0.3",
  "private": false,
  "description": "A TypeScript SDK for The Blue Alliance API",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts --max-warnings 0",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.1",
    "eslint": "^9.29.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.1",
    "ts-jest": "^29.4.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "zod": "^3.25.67"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yeti-Robotics/polar-edge-analytics.git"
  }
}
